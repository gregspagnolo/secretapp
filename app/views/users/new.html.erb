<h4>New Users</h4>
<form action="/users" method="post">
	<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
		<p>Email: <input type="email" id="email" name="user[email]"></p>
		<p>Name: <input type="text" id="name" name="user[name]"></p>
		<p>Password: <input type="password" id="password" name="user[password]"></p>
		<p>Password Confirmation: <input type="password" id="password_confirmation" name="user[password_confirmation]"></p>
	<input type="submit" value="Create User">
</form>